<br><br>
<center>
<div class="container">
<p id="notice"><%= notice %></p>

<h1>Key Result</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Medida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @key_results.each do |key_result| %>
      <tr>
        <td><%= key_result.user.name %></td>
        <td><%= key_result.description %></td>
        <td><%= key_result.period_value %></td>
        <td><%= key_result.tiempo.name %></td>
        <td><%= link_to 'Show', key_result %></td>
        <td><%= link_to 'Edit', edit_key_result_path(key_result) %></td>
        <td><%= link_to 'Destroy', key_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %><br>
   
  </tbody>
</table>

<br>

<%= link_to 'New key_result', new_key_result_path %>
</div></center  >
